    </UserControl.Resources>
    <DockPanel>
        <DockPanel DockPanel.Dock="Bottom" Margin="5 5">
            <Panel DockPanel.Dock="Bottom">
                <DockPanel ZIndex="1" HorizontalAlignment="Stretch">
                    <StackPanel Orientation="Horizontal" Spacing="5" DockPanel.Dock="Left" HorizontalAlignment="Left">
                        <ToggleSplitButton Name="Volume" IsCheckedChanged="Volume_OnIsCheckedChanged">
                            <Panel>
                                <PathIcon IsVisible="{CompiledBinding !#Volume.IsChecked}"
                                          Data="{StaticResource speaker_regular}" />
                                <PathIcon IsVisible="{CompiledBinding #Volume.IsChecked}"
                                          Data="{StaticResource speaker_off_regular}" />
                            </Panel>
                            <ToggleSplitButton.Flyout>
                                <Flyout>
                                    <StackPanel Orientation="Vertical" HorizontalAlignment="Center">
                                        <Slider Height="100" Name="VolumeSlider" Maximum="100" Margin="5"
                                                IsSnapToTickEnabled="True"
                                                TickFrequency="1" ValueChanged="VolumeSlider_OnValueChanged"
                                                Value="100" Orientation="Vertical" />
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <TextBlock VerticalAlignment="Center"
                                                       Text="{CompiledBinding #VolumeSlider.Value}" />
                                            <TextBlock VerticalAlignment="Center" Text="%" />
                                        </StackPanel>
                                    </StackPanel>

                                </Flyout>
                            </ToggleSplitButton.Flyout>
                        </ToggleSplitButton>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Spacing="5" DockPanel.Dock="Right" HorizontalAlignment="Right">
                        <ToggleButton Name="Repeat">
                            <Panel>
                                <PathIcon IsVisible="{CompiledBinding !#Repeat.IsChecked}"
                                          Data="{StaticResource arrow_repeat_all_off_regular}" />
                                <PathIcon IsVisible="{CompiledBinding #Repeat.IsChecked}"
                                          Data="{StaticResource arrow_repeat_all_regular}" />
                            </Panel>
                        </ToggleButton>
                    </StackPanel>
                </DockPanel>
                <StackPanel ZIndex="0" Orientation="Horizontal" HorizontalAlignment="Center">
                    <Button Name="Rewind" Click="Rewind_OnClick">
                        <PathIcon Data="{StaticResource rewind_regular}" />
                    </Button>
                    <ToggleButton Name="PlayPause" IsCheckedChanged="PlayPause_OnIsCheckedChanged">
                        <Panel>
                            <PathIcon IsVisible="{CompiledBinding !#PlayPause.IsChecked}"
                                      Data="{StaticResource play_regular}" />
                            <PathIcon IsVisible="{CompiledBinding #PlayPause.IsChecked}"
                                      Data="{StaticResource pause_regular}" />
                        </Panel>
                    </ToggleButton>
                    <Button Name="Stop" Click="Stop_OnClick">
                        <PathIcon Data="{StaticResource stop_regular}" />
                    </Button>
                    <Button Name="FastForward" Click="FastForward_OnClick">
                        <PathIcon Data="{StaticResource fast_forward_regular}" />
                    </Button>
                </StackPanel>
            </Panel>
            <Panel DockPanel.Dock="Bottom">
                <TextBlock Name="CurrentPos" Text="00:00:00.00000" DockPanel.Dock="Left" HorizontalAlignment="Left" />
                <TextBlock Name="FullPos" DockPanel.Dock="Right" Text="00:00:00.00000" HorizontalAlignment="Right" />
            </Panel>
            <Slider Name="PosSlider" Maximum="1000" ValueChanged="PosSlider_OnValueChanged" />
        </DockPanel>
        <StackPanel VerticalAlignment="Center">
            <Viewbox Width="150" Height="150" Margin="10">
                <Panel>
                    <Image Name="TitleImage" IsVisible="{CompiledBinding !!$self.Source}" />
                    <Border IsVisible="{CompiledBinding !#TitleImage.IsVisible}" CornerRadius="2"
                            Background="{CompiledBinding $self.BorderBrush}"
                            BorderBrush="{DynamicResource SystemChromeMediumColor}" BorderThickness="2">
                        <PathIcon Data="{StaticResource music_regular}"
                                  Foreground="{DynamicResource SystemChromeHighColor}" />
                    </Border>
                </Panel>
            </Viewbox>
            <TextBlock Name="PreviewTitle" IsVisible="{CompiledBinding !!$self.Text}" TextAlignment="Center"
                       HorizontalAlignment="Center" FontSize="26" TextWrapping="Wrap" FontWeight="Bold" />
            <TextBlock Text="{x:Static lang:Lang.PreviewAudio_UnknownTitle}"
                       IsVisible="{CompiledBinding !#PreviewTitle.IsVisible}"
                       TextAlignment="Center" HorizontalAlignment="Center" FontSize="26" TextWrapping="Wrap"
                       FontWeight="Bold" />
            <TextBlock Name="PreviewArtist" IsVisible="{CompiledBinding !!$self.Text}" TextAlignment="Center"
                       HorizontalAlignment="Center" FontSize="18" TextWrapping="Wrap" />
            <TextBlock Text="{x:Static lang:Lang.PreviewAudio_UnknownArtist}"
                       IsVisible="{CompiledBinding !#PreviewArtist.IsVisible}"
                       TextAlignment="Center" HorizontalAlignment="Center" FontSize="18" TextWrapping="Wrap" />
        </StackPanel>
    </DockPanel>
</UserControl>